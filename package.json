{
  "name": "shoppinglist",
  "version": "1.0.0",
  "description": "Shopping list with auth",
  "main": "server.js",
  "scripts": {
    "client-install":"npm install --prefix client",
    "start": "node server.js",
    "server":"nodemon server.js",
    "client":"npm start --prefix client",
    "client-server":"concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild":"NPM_CONFIG_PRODUCTION=false npm istall --prefix client && npm run build --prefix client"
  },
  "author": "Desmond",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^5.0.0",
    "express": "^4.17.1",
    "mongoose": "^5.7.9"
  }
}
